    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 WorldViewProj;

void vs_main(
     float4 position    : POSITION,
     float3 normal      : NORMAL,
     float2 texcoords   : TEXCOORD0,

 out float4 oPosition   : POSITION,   // clip space
 out float2 oTCoords    : TEXCOORD0,
 out float3 oNormal     : TEXCOORD1,
 out float4 oLightClipPos: TEXCOORD2
 )
{
   float dx = 100.0;
   float dy = 100.0;
   float dz = 1000.0;

   float x1 = -dx;
   float x2 =  dx;
   
   float y1 = -dy;
   float y2 =  dy;
   
   float z1 = -dz;
   float z2 =  dz;

   float4x4 Proj = {
       2/(x2-x1), 0,         0,         (x1+x2)/(x1-x2),
       0,         2/(y2-y1), 0,         (y1+y2)/(y1-y2),
       0,         0,         1/(z2-z1), z1/(z1-z2),
       0,         0,         0,         1 };

   float4x4 LightWorldViewProj = mul(Proj, mul(View, World));
   oLightClipPos = mul(LightWorldViewProj, position);




    oNormal = normalize(normal);    
    oPosition = mul(WorldViewProj, position);
    oTCoords = texcoords;
    // De cette manire, oPosition.z / oPosition.w est toujours gal  1 et le pixel
    // est toujours le plus au fond possible du z buffer.
    //oPosition.w = 1.0000001*oPosition.z;
}
]]></RmShaderCode></RmHLSLShader>
    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float3 MainLightDir;
float4 Color;
float Intensity;
float Intensity2;

float Sharpness;
float MinIntensity;
float MaxIntensity;
float Ambient;

sampler2D DiffuseMap;

#define PI 3.1415

#define LIGHTING_MODEL 2

float4 ps_main(
 float2 tcoords    : TEXCOORD0,
 float3 normal     : TEXCOORD1,
 float4 oLightClipPos: TEXCOORD2
) : COLOR0
{
    normal = normalize(normal);
    float3 light = -normalize(MainLightDir);

    float2 asinnl = asin(float2(normal.y, light.y));

    float teta = 0.5*PI - asinnl.x;
    float S = 0.5*PI - asinnl.y;
    float gamma = acos(dot(normal, light));

    float3 cosgts = cos(float3(gamma, teta, S));
    
    float cosg = cosgts.x;
    float cost = cosgts.y;
    float coss = cosgts.z;
    
    float cosg2 = cosg * cosg;
    float coss2 = coss * coss;
    
#if LIGHTING_MODEL == 0 // LIGHTING_MODEL_FOGGY
        
        float teta2 = PI - asin(normal.y);
        float L = (1.0 + 2.0 * sin(teta2)) / 3.0;
        L = L*Intensity;
        
#elif LIGHTING_MODEL == 1 // LIGHTING_MODEL_CLOUDY

cost = abs(cost);
coss = cost = 1.0;

        float num = (0.526 + 5.0 * exp(-1.5 * gamma)) * 
            (1.0 - min(1.0, exp(-0.8 / cost)));
        
        float denum = (0.526 + 5.0 * exp(-1.5 * S)) *
            (1.0 - min(1.0, exp(-0.8)));
        
        float maxNum = (0.526 + 5.0 * exp(-1.5 * 0.0)) * 
            (1.0 - min(1.0, exp(-0.8 / coss)));
            
        float maxValue = maxNum / denum;
        float L = (num / denum)*Intensity;;

        float teta2 = PI - asin(normal.y);
        //float L = (1.0 + 2.0 * sin(teta2)) / 3.0;
        L = L*(1.0 + 2.0 * sin(teta2)) / 3.0;

#elif LIGHTING_MODEL == 2 // LIGHTING_MODEL_CLEAR_SKY

cost = abs(cost);
coss = cost = 1.0;

        float num = (0.91 + 10.0 * exp(-3.0 * gamma) + 0.45 * cosg2) * 
            (1.0 - min(1.0, exp(-0.32 / cost)));
        
        float denum = (0.91 + 10.0 * exp(-3.0 * S) + 0.45 * coss2) *
            (1.0 - min(1.0, exp(-0.32)));
        
        float maxNum = (0.91 + 10.0 * exp(-3.0 * 0.0) + 0.45 * 1.0) * 
            (1.0 - min(1.0, exp(-0.32 / coss)));
            
        float maxValue = maxNum / denum;
        float L = (num / denum)*Intensity;// / maxValue;

        float teta2 = PI - asin(normal.y);
   L = L*(1.0 + 2.0 * sin(teta2)) / 3.0;
//L = 0.5f * (L + (0.5f * dot(light, normal) + 0.5f));

#elif LIGHTING_MODEL == 3 // LIGHTING_MODEL_CLEAR_SKY
   float L = (0.5f * dot(light, normal) + 0.5f)* Intensity2;
   //float L = max(0.0, dot(light, normal)) * Intensity2;

#else
#error
#endif

    L = max(MinIntensity, min(L + Ambient, MaxIntensity));
    //L = pow(L, Sharpness);
    L = 1.0 - pow(1.0 - L, Sharpness);

    //return float4(L * Color.rgb, 1.0);
    return float4(L * Color.rgb, 1.0) * tex2D(DiffuseMap, tcoords);
    //return float4(normal, 1.0);
}

]]></RmShaderCode>
