    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 WorldViewProj;
float4x4 World;
float4x4 View;

void vs_main(
       float4 position  : POSITION,
      
   out float4 oPosition : POSITION,
   out float2 oDepth    : TEXCOORD0
   )
{
   float dx = 100.0;
   float dy = 100.0;
   float dz = 1000.0;

   float x1 = -dx;
   float x2 =  dx;
   
   float y1 = -dy;
   float y2 =  dy;
   
   float z1 = -dz;
   float z2 =  dz;

   float4x4 Proj = {
       2/(x2-x1), 0,         0,         (x1+x2)/(x1-x2),
       0,         2/(y2-y1), 0,         (y1+y2)/(y1-y2),
       0,         0,         1/(z2-z1), z1/(z1-z2),
       0,         0,         0,         1 };

   float4x4 WorldViewProj = mul(Proj, mul(View, World));

   oPosition = mul(WorldViewProj, position);
   oDepth = oPosition.zw;
}
]]></RmShaderCode>
     <RmShaderConstant NAME="View" API="" REGISTER="4"/>
     <RmShaderConstant NAME="World" API="" REGISTER="0"/>
    </RmHLSLShader>
    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 ps_main(
   float2 depth    : TEXCOORD0
) : COLOR0
{   
    float z = depth.x / depth.y;
    return float4(z, 0.0, 0.0, 0.0);
}



]]></RmShaderCode></RmHLSLShader>
